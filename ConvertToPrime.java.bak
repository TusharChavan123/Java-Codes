import java.util.Scanner;
class ConvertToPrime 
{
	public static void main(String[] args) 
	{
	Scanner sc=new Scanner(System.in);
	int num=sc.nextInt();
	int den=2;
	while(den<=num/2)
		{
		if (num%den==0)
		{
			break;
		
		}
		den++;
		}
		if(den>num/2)
		{
			System.out.println("Prime");
		}
		else
		{
			int rem=den;
		
		}
	}
}
